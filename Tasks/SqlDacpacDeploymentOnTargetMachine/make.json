{
    "externals": {
       "nugetv2": [
            {
                "name": "N.TaskModule.SqlUtility",
                "version": "1.0.0",
                "repository": "https://www.powershellgallery.com/api/v2/",
                "cp": [
                    {
                      "source": "*.ps*",
                      "dest":"ps_modules/N.TaskModule.SqlUtility"
                    }
                ]
            },
			{
                "name": "ThreadJob",
                "version": "2.0.3",
                "repository": "https://www.powershellgallery.com/api/v2/",
                "cp": [
                    {
                        "source": [
                            "*.dll",
                            "*.psd1"
                        ],
                      "dest":"ps_modules/ThreadJob"
                    }
                ]
            },
            {
                "name": "VstsTaskSdk",
                "version": "0.8.2",
                "repository": "https://www.powershellgallery.com/api/v2/",
                "cp": [
                    {
                        "source": [
                            "*.dll",
                            "*.ps1",
                            "*.psd1",
                            "*.psm1",
                            "lib.json",
                            "Strings"
                        ],
                        "dest": "ps_modules/VstsTaskSdk/",
                        "options": "-R"
                    }
                ]
            }
        ]
    },
    "cp": [
        {
            "source": [
                "*.sql"
            ]
        }
    ]
}